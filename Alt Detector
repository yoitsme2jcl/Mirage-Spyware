-- Functions (Advanced programming stuff!!!)
function isAltAccount(playerName)
    -- The ultimate test
    if string.len(playerName) < 10 then
        print(playerName .. " is an alt! ðŸš¨ðŸš¨ðŸš¨")
        return true
    elseif playerName:find("123") then
        print(playerName .. " has numbers, must be fake!!! ðŸš¨ðŸš¨ðŸš¨")
        return true
    elseif playerName:lower():find("alt") then
        print(playerName .. " literally says 'alt' bro. ðŸš¨ðŸš¨ðŸš¨")
        return true
    else
        print(playerName .. " is clear. For now... ðŸ‘€")
        return false
    end
end

-- Main Logic (DO NOT EDIT!!! ðŸš¨ðŸš¨ðŸš¨)
function detectAlts()
    print("Starting alternate account detection...")
    totalPlayers = totalPlayers + 1 -- just in case more players join while weâ€™re coding
    for i = 1, totalPlayers do
        local playerName = "Player" .. i -- Player Fetching System
        print("Checking player: " .. playerName)
        if isAltAccount(playerName) then
            table.insert(alts, playerName)
        end
    end
end

-- Run the script (Mega OP Feature ðŸš€)
